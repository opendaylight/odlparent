                <artifactId>features-odlparent</artifactId>
                <version>1.8.0-SNAPSHOT</version>
                <classifier>features</classifier>
                <type>xml</type>
            </dependency>
            <dependency>
                <groupId>org.opendaylight.odlparent</groupId>
                <artifactId>odl-akka-all</artifactId>
                <version>1.8.0-SNAPSHOT</version>
