<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>org.opendaylight.odlparent</groupId>
        <artifactId>bnd-parent</artifactId>
        <version>14.2.1</version>
        <relativePath>../../bnd-parent/pom.xml</relativePath>
    </parent>

    <artifactId>dagger-example</artifactId>
    <packaging>jar</packaging>
    <name>OpenDaylight :: Dagger :: Example</name>
    <description>Example of use of OpenDaylight Dagger support</description>

    <properties>
        <!-- Dagger-generated classes do not have javadocs -->
        <doclint>all,-missing</doclint>
        <maven.javadoc.failOnWarnings>true</maven.javadoc.failOnWarnings>

        <!-- Do not publish this artifact -->
        <maven.deploy.skip>true</maven.deploy.skip>

        <!-- Opt-in to class path duplicate checks -->
        <duplicate-finder.skip>false</duplicate-finder.skip>
        <!-- Opt-in to @Var world -->
        <odl.ep.var>ERROR</odl.ep.var>

        <!-- Needed to suppress 'URF_UNREAD_FIELD' violation -->
        <spotbugs.onlyAnalyze>!org.opendaylight.odlparent.dagger.example.dagger.DaggerExampleComponent$ExampleComponentImpl</spotbugs.onlyAnalyze>
    </properties>

    <dependencies>
        <dependency>
            <groupId>com.google.dagger</groupId>
            <artifactId>dagger</artifactId>
            <optional>true</optional>
        </dependency>
        <dependency>
            <groupId>com.google.errorprone</groupId>
            <artifactId>error_prone_annotations</artifactId>
        </dependency>
        <dependency>
            <groupId>com.guicedee.services</groupId>
            <artifactId>javax.inject</artifactId>
            <optional>true</optional>
        </dependency>
        <dependency>
            <groupId>jakarta.inject</groupId>
            <artifactId>jakarta.inject-api</artifactId>
            <optional>true</optional>
        </dependency>
        <dependency>
            <groupId>org.eclipse.jdt</groupId>
            <artifactId>org.eclipse.jdt.annotation</artifactId>
        </dependency>
        <dependency>
            <groupId>org.opendaylight.odlparent</groupId>
            <artifactId>dagger</artifactId>
            <optional>true</optional>
        </dependency>
    </dependencies>
</project>
