[access]
	inheritFrom = All-Projects
[receive]
	requireChangeId = true
[project]
	description = odlparent
[access "refs/*"]
	owner = group ldap/gerrit-odlparent-committers
	read = group GitHub Replication
[submit]
	action = rebase if necessary
[allowSuperproject "docs"]
	matching = refs/heads/master:refs/heads/master
[allowSuperproject "releng/autorelease"]
	matching = refs/heads/stable/beryllium:refs/heads/stable/beryllium
